{
  "extends": "astro/tsconfigs/strict",
  "include": ["src"],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@@/*": ["./*"],
      "@src/*": ["src/*"],
      "@typings/*": ["src/types/*"],
      "@public/*": ["public/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@partials/*": ["src/components/partials/*"]
    }
  }
}
